<div class="container">
    <div class="row" style="height: 140px"></div>
    <h3>View All Posts:</h3>
    <ul id="post-list" class="list-group">
        {{#each post}}
        <li data-id="{{this.id}}" class="list-group-item">
        <span><h4>{{this.title}}</h4><p>Posted by: {{this.User.firstName}} {{this.User.lastName}}</p>
        </span>
        <p>{{this.content}}</p>
        <a href="post/{{this.id}}">View & Critique</a>
        </li>
        {{/each}}
    </ul>

</div>
